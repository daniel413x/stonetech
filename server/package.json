{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon index.ts",
    "test": "cross-env NODE_ENV=test nodemon index.ts",
    "compileCommon": "babel db --extensions \".ts,.js\" --out-dir ./lib/db && babel ./utils --extensions \".ts,.js\" --out-dir ./lib/utils && babel ./migrations --extensions \".ts,.js\" --out-dir ./lib/migrations",
    "babel": "npm run compileCommon && babel ./seeders --extensions \".ts,.js\" --out-dir ./lib/seeders",
    "babel:test": "npm run compileCommon && babel ./seeders_test --extensions \".ts,.js\" --out-dir ./lib/seeders_test",
    "dev:migrate": "npm run babel && npx cross-env NODE_ENV=development sequelize-cli db:migrate && npx cross-env NODE_ENV=development sequelize-cli db:seed:all",
    "dev:migrate:undo": "npx cross-env NODE_ENV=development sequelize-cli db:migrate:undo",
    "dev:migrate:undo:all": "npx cross-env NODE_ENV=development sequelize-cli db:migrate:undo:all",
    "test:migrate": "npm run babel:test && npx cross-env NODE_ENV=test sequelize-cli db:migrate && npx cross-env NODE_ENV=test sequelize-cli db:seed:all",
    "test:migrate:undo": "npx cross-env NODE_ENV=test sequelize-cli db:migrate:undo",
    "test:migrate:undo:all": "npx cross-env NODE_ENV=test sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.45",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.16",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.0",
    "sequelize-typescript": "^2.1.3",
    "uuid": "^8.3.2"
  }
}
